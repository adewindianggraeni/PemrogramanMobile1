<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/iphone_8_1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true"
    android:background="@drawable/layer">


    <TextView
        android:id="@+id/hi_windi"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        android:text="@string/hi_windi"
        android:textAppearance="@style/hi_windi"
        android:gravity="top" />

    <View
        android:id="@+id/account"
        android:layout_width="35dp"
        android:layout_height="34dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="30dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/acc"
        />

    <TextView
        android:id="@+id/ayok_pesan_"
        android:layout_width="193dp"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="55dp"
        android:text="@string/ayok_pesan_"
        android:textAppearance="@style/ayok_pesan_"
        android:gravity="top" />

    <View
        android:id="@+id/sarapan"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="145dp"
        android:background="@drawable/sarapan"
        />

    <View
        android:id="@+id/makan_siang"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="124dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="145dp"
        android:background="@drawable/makan_siang"
        />

    <View
        android:id="@+id/cepat_saji"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="221dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="145dp"
        android:background="@drawable/cepat_saji"
        />

    <View
        android:id="@+id/minuman"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="318dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="145dp"
        android:background="@drawable/minuman"
        />

    <TextView
        android:id="@+id/sekitarmu"
        android:layout_width="69dp"
        android:layout_height="18dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="29dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="249dp"
        android:text="@string/sekitarmu"
        android:textAppearance="@style/sekitarmu"
        android:gravity="top"
        />


</RelativeLayout>