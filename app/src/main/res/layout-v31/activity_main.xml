<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/iphone_8_1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true"
    android:background="@drawable/layer">


    <TextView
        android:id="@+id/hi_windi"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="27dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        android:text="@string/hi_windi"
        android:textAppearance="@style/hi_windi"
        android:gravity="top" />

    <View
        android:id="@+id/account"
        android:layout_width="35dp"
        android:layout_height="34dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="30dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/acc"
        />

    <TextView
        android:id="@+id/ayok_pesan_"
        android:layout_width="193dp"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="27dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="55dp"
        android:text="@string/ayok_pesan_"
        android:textAppearance="@style/ayok_pesan_"
        android:gravity="top" />
</RelativeLayout>